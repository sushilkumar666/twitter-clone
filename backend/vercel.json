{
    "version": 2,
    "builds": [
      { "src": "/api/*", "use": "@vercel/node" },
      { "src": "/(.*)", "use": "@vercel/static" }
    ],
    "routes": [
      { "handle": "filesystem" },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1"
      }
    ]
  }